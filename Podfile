# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'SpotSwap' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for SpotSwap
  pod 'Firebase/Core'
  pod 'Firebase/Auth’
  pod 'Firebase/Database’
  pod 'SnapKit'
  pod 'Firebase/Storage'
  pod 'Toucan'
  pod 'Kingfisher'
  pod 'ImagePicker'
  pod 'Pastel'
  pod 'SwiftGifOrigin'
  target 'SpotSwapTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'SpotSwapUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
